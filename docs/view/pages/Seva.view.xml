<mvc:View xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" 
	xmlns:core="sap.ui.core">
	<ScrollContainer
		height="calc(100% - 10em)"
		horizontal="false"
		vertical="true">
	<List
		headerText="{i18n>headerTxtSeva}">
		<items>
			<ObjectListItem
				title="{i18n>sevaTxt1}"
				number="60"
				numberUnit="INR">
			
				<attributes>
					<ObjectAttribute text="{i18n>sevaSubText3}" />
					<ObjectAttribute text="{i18n>sevaSubText1}" />
				</attributes>
			</ObjectListItem>
			
			<ObjectListItem
				title="{i18n>sevaTxt2}"
				number="30"
				numberUnit="INR">
			
				<attributes>
					<ObjectAttribute text="{i18n>sevaSubText1}" />
				</attributes>
			</ObjectListItem>
			
				<ObjectListItem
				title="{i18n>sevaTxt3}"
				number="30"
				numberUnit="INR">
			
				<attributes>
					<ObjectAttribute text="{i18n>sevaSubText2}" />
				</attributes>
			</ObjectListItem>
			
				
		</items>
	</List>
</ScrollContainer>
</mvc:View>