							<Text text="{i18n>bankAddress}"/>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>bankAccountName}">
						<f:fields>
							<Text text="SRI MAHALAKSHMI TEMPLU TRU"/>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>bankAccountNumber}">
						<f:fields>
							<Text text="54053471658"/>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>IFSCCode}">
						<f:fields>
							<Text text="SBMY0040238"/>
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
		</f:formContainers>
	</f:Form>
	<VBox class="sapUiSmallMargin">
		<Text text="{i18n>noteTxt1}"></Text>
