				</f:formElements>
			</f:FormContainer>
		
		<f:FormContainer title="{i18n>mapHeaderTxt}">
				<f:formElements>
					<f:FormElement>
						<f:fields>
							<Link text="{i18n>mapNavTxt}" class="sapUiSmallMarginTop" emphasized="true" href="https://www.google.co.in/maps/dir/Tumkur,+Karnataka,+India/Anupanhalli+Mahalaxmi+Temple,+Tumakuru,+Karnataka+572122,+India/@13.2676425,77.0468044,12z/data=!3m1!4b1!4m14!4m13!1m5!1m1!1s0x3bb02c3b632e23b9:0xe15fb239e9d737bb!2m2!1d77.1139984!2d13.3391677!1m5!1m1!1s0x3bafd5ecef815e9d:0xb8df6dbb1b149f53!2m2!1d77.12351!2d13.200339!3e0" />
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
			
		<f:FormContainer title="{i18n>railHeaderTxt}">
				<f:formElements>
					<f:FormElement>
						<f:fields>
							<Text text="{i18n>railTxt1}" />
						</f:fields>
					</f:FormElement>
					<f:FormElement>
						<f:fields>
							<Text text="{i18n>railTxt2}" />
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
			
			<f:FormContainer title="{i18n>townHeaderTxt}">
